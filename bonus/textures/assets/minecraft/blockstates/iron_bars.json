{
    "multipart": [
        {   "apply": { "model": "block/iron_bars/iron_bars_post_ends" }},


        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_post" }
        },


        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side" }
        },

        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap", "y": 90 }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side", "y": 90 }
        },

        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap" }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side", "y": 180 }
        },

        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "block/iron_bars/iron_bars_cap", "y": 90}
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side", "y": 270 }
        },


        {   "when": { "north": true, "east": true, "south": false, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side" }
        },
        {   "when": { "north": false, "east": true, "south": true, "west": false },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side", "y": 90 }         
        },
        {   "when": { "north": false, "east": false, "south": true, "west": true },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side", "y": 180 }        
        },
        {   "when": { "north": true, "east": false, "south": false, "west": true },
            "apply": { "model": "block/iron_bars/iron_bars_cap_side", "y": 270 }        
        },
            
        
        {   "when": { "OR": [ { "east": false }, { "west": false } ] },
            "apply": { "model": "block/iron_bars/iron_bars_cap" }
        },
        {   "when": { "OR": [ { "south": false }, { "north": false } ] },
            "apply": { "model": "block/iron_bars/iron_bars_cap", "y": 90}
        }, 


        {   "when": { "north": true },
            "apply": { "model": "block/iron_bars/iron_bars_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/iron_bars/iron_bars_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/iron_bars/iron_bars_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "block/iron_bars/iron_bars_side_alt", "y": 90 }
        }
    ]

 }     